  "private": true,
  "scripts": {
    "dev": "nodemon --exec \"npx tsx server.ts\" --watch server.ts --watch src --ext ts,tsx,js,jsx",
    "build": "npm run db:generate-schema && node scripts/safe-migrate.js && node scripts/prisma-generate.js && next build",
    "start": "cross-env NODE_ENV=production tsx server.ts",
    "lint": "next lint",
    "postinstall": "node scripts/prisma-generate.js",
    "db:push": "npm run db:generate-schema && prisma db push",
    "db:generate": "npm run db:generate-schema && node scripts/prisma-generate.js",
    "db:generate-schema": "node scripts/generate-schema.js",
    "db:migrate": "npm run db:generate-schema && prisma migrate dev",
    "db:reset": "npm run db:generate-schema && prisma migrate reset",
    "db:check": "node scripts/update-production-schema.js",
    "test:e2e:setup-data": "tsx tests/e2e/utils/test-setup.ts setup",
    "test:e2e:cleanup-data": "tsx tests/e2e/utils/test-setup.ts cleanup",
    "test:e2e:reset-db": "tsx tests/e2e/utils/test-setup.ts reset",
    "prepare": "husky",
    "security:check": "npm audit --audit-level=moderate",
    "security:fix": "npm audit fix",
    "security:report": "node scripts/security-report.js",
    "copyright:check": "node scripts/check-copyright.js",
    "favicon:generate": "node scripts/generate-favicons.js",
    "favicon:test": "node scripts/test-favicon.js"
